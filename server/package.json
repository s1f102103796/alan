{
  "name": "server",
  "version": "0.1.0",
  "scripts": {
    "predev": "npm run generate && npm run migrate:deploy",
    "dev": "run-p dev:*",
    "dev:build": "node ./scripts/build.dev.js",
    "dev:server": "node-dev --respawn index.js",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "prebuild": "npm run generate && npm run migrate:deploy",
    "build": "node ./scripts/build.prod.js",
    "generate": "run-s generate:prisma generate:aspida generate:frourio",
    "generate:aspida": "aspida -c aspida-prod.config.js",
    "generate:frourio": "frourio",
    "generate:prisma": "prisma generate",
    "migrate:dev": "prisma migrate dev && npx prisma db seed",
    "migrate:dev:createonly": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy && npx prisma db seed",
    "migrate:reset": "prisma migrate reset",
    "start": "node --enable-source-maps index.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "node -r esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@apollo/client": "^3.8.8",
    "@aspida/fetch": "^1.14.0",
    "@aws-sdk/client-s3": "^3.425.0",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/etag": "^5.1.0",
    "@fastify/helmet": "^11.0.0",
    "@napi-rs/canvas": "^0.1.44",
    "@prisma/client": "^5.1.1",
    "aspida": "^1.14.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "firebase-admin": "^11.10.1",
    "graphql": "^16.8.1",
    "is-binary-path": "^2.1.0",
    "localtunnel": "^2.0.2",
    "openai": "^4.24.2",
    "openapi2aspida": "^0.23.2",
    "simple-git": "^3.21.0",
    "velona": "^0.8.0",
    "zod": "^3.22.2",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^20.4.10",
    "commonTypesWithClient": "file:./commonTypesWithClient",
    "esbuild": "^0.18.18",
    "esbuild-node-externals": "^1.8.0",
    "esbuild-register": "^3.4.2",
    "frourio": "^1.0.0",
    "node-dev": "^8.0.0",
    "notios": "^0.4.1",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0 <21"
  }
}
